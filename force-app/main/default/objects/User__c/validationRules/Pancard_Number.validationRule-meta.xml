<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Pancard_Number</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(   REGEX( P_PAN_Number__c , &quot;[A-Z]{5}[0-9]{4}[A-Z]{1}&quot;) )</errorConditionFormula>
    <errorMessage>Invalid Pan Number</errorMessage>
</ValidationRule>
